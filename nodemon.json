{
  "watch": [
    "server.js",
    "app.js",
    "config/",
    "controllers/",
    "middleware/",
    "models/",
    "routes/",
    "utils/"
  ],
  "ext": "js,json",
  "ignore": [
    "node_modules/",
    "logs/",
    "public/",
    "views/",
    "*.log"
  ],
  "exec": "node server.js",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 1000,
  "verbose": true,
  "restartable": "rs"
}
